charge <- function(s = 0.1,t = 1,h = 0,m = 0,d = 1){
#Calculate tariff due to duration of the connection in cost units per second [cu/sec]
tariff_duration_seg <- (BW_eff(s,t,h,m) - BW_prima(s,t,h,m)*m)/(1000*h) #Normalized
#Calculate tariff due to duration of the connection in cost units per month [cu/month]
tariff_duration_month <- tariff_duration_seg*2628000
#Calculate tariff due to volume of information transferred in cost units per Megabit [cu/Mbit]
tariff_usage_Mbit  <- BW_prima(s,t,h,m)/(1000*h) #Normalized
#Calculate tariff due to volume of information transferred in cost units per MegaByte [cu/MByte]
tariff_usage_MByte <- tariff_usage_Mbit*8
# Calculate the volume of information transferred over d = 1 month in Megabits [Mbits]
Volume_info_Mbits <- m*2628000*d
# Calculate the volume of information transferred over d = 1 month in MBytes [MBytes]
Volume_info_MBytes <- Volume_info_Mbits*d/8
#------------------------- Charge Calculation----------------------------------------------------
#Calculate charge component due to a duration of one month  [cu]
charge_duration_seg <- tariff_duration_seg*2628000
#Calculate charge component due to a duration of one month  [cu]
charge_duration_month <- tariff_duration_month*d
#Calculate charge due to volume of information transferred in in one month [cu]
charge_usage_Mbit  <- tariff_usage_Mbit*Volume_info_Mbits
#Calculate charge due to volume of information transferred in in one month [cu]
charge_usage_MByte <- tariff_usage_MByte*Volume_info_MBytes
# Calculate the total charge
charge1 <- charge_duration_seg + charge_usage_Mbit
charge2 <- charge_duration_month + charge_usage_MByte
bill <- data.frame("Charge" = round(charge2,1),
"Duration" = round(charge_duration_month,1),
"Usage" = round(charge_usage_MByte,1),
"Traffic" = round(Volume_info_MBytes,1)
)
return(bill)
}
